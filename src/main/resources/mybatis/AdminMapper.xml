<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cc.admin.dao.AdminMapper">
    <resultMap type="Admin" id="AdminMap">
        <id column="id" property="id" />
        <result column="user_name" property="username"/>
        <result column="cell_phone" property="cellPhone"/>
        <result column="register_time" property="registerTime"/>
    </resultMap>

    <select id="queryUsers" resultMap="AdminMap">
    	SELECT 
    		* 
    	FROM 
    		admin 
    </select>
    
    <insert id="addUser" parameterType="com.cc.admin.dto.Admin" useGeneratedKeys="true" keyProperty="id">
    	INSERT INTO 
        	admin 
        	(cell_phone,user_name,password,register_time,role,card)
        VALUES 
        	(#{cellPhone},#{username},#{password},#{registerTime},#{role},#{card})
    </insert>

	

</mapper>